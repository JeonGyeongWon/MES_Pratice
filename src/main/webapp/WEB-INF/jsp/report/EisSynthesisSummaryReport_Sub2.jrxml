	<queryString>
		<![CDATA[SELECT '매출매입 실적' AS TITLE
,A.RN || '월' AS STANDARDMONTH
      ,ORG_ID AS ORGID
      ,COMPANY_ID AS COMPANYID
      ,SEARCH_YEAR AS SEARCHYEAR
      ,CB_EIS_PKG.CB_EIS_SUMMARY_FIND(ORG_ID
                                     ,COMPANY_ID
                                     ,SEARCH_YEAR
                                     ,A.RN
                                     ,1
                                     ,2) AS SALESAMOUNT
      ,CB_EIS_PKG.CB_EIS_SUMMARY_FIND(ORG_ID
                                     ,COMPANY_ID
                                     ,SEARCH_YEAR
                                     ,A.RN
                                     ,2
                                     ,2) AS PURCHASEAMOUNT
  FROM (
SELECT LPAD(LEVEL, 2, '0') AS RN
      ,$P{ORGID} AS ORG_ID
      ,$P{COMPANYID} AS COMPANY_ID
      ,$P{SEARCHYEAR} AS SEARCH_YEAR
  FROM DUAL
CONNECT BY LEVEL <= 12
) A]]>
	</queryString>
