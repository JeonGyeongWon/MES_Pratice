      ,ORG_NAME AS ORGNAME
      ,COMPANY_ID AS COMPANYID
      ,SHIP_NO AS SHIPNO
      ,SHIP_SEQ AS SHIPSEQ
      ,SHIP_DATE AS SHIPDATE
