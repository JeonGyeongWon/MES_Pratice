	<queryString>
		<![CDATA[SELECT '매입비율 계획 대비 실적' AS TITLE
      ,A.RN || '월' AS STANDARDMONTH
      ,ORG_ID AS ORGID
      ,COMPANY_ID AS COMPANYID
      ,SEARCH_YEAR AS SEARCHYEAR
      ,CB_EIS_PKG.CB_EIS_SUMMARY_FIND(ORG_ID
