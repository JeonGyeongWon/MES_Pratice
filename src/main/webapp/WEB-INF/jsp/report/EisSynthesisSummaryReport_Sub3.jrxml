                                     ,COMPANY_ID
                                     ,SEARCH_YEAR
                                     ,A.RN
                                     ,3
                                     ,2) AS RESULTAMOUNT
  FROM (
SELECT LPAD(LEVEL, 2, '0') AS RN
      ,$P{ORGID} AS ORG_ID
      ,$P{COMPANYID} AS COMPANY_ID
      ,$P{SEARCHYEAR} AS SEARCH_YEAR
  FROM DUAL
CONNECT BY LEVEL <= 12
) A]]>
